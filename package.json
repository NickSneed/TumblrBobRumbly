    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "cls && curl -o tumblr/local.html https://bobrumbly.com/ && npx stylelint src/**/*.scss && npm run compile:sass && eslint src && npm run compile:js",
